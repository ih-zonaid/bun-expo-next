{
  "name": "ih-starter",
  "type": "module",
  "private": true,
  "scripts": {
    "expo": "cd apps/expo && expo start",
    "dev": "cd apps/nextjs && bun dev",
    "db:push": "export $(grep -v '^#' apps/nextjs/.env | grep -v '^$' | sed 's/#.*//' | xargs) && cd packages/server && bun run db:push"
  },
  "devDependencies": {},
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {}
}